<h1>Add A New Tile</h1>

<%= form_for :tile, url: tiles_path do |f| %>
	<p>
		<%= f.label :element %><br>
		<%= f.text_field :element %>
	</p>

	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</p>

	<p>
		<%= f.check_box :is_html5 %>
	</p>

	<p>
		
	</p>

	<p>
		<%= f.submit "Create Tile" %>
	</p>
<% end %>